branches: master
cabal-check: False

-- GADT docs
haddock: >=8.6

hlint: True
hlint-job: 8.8.3
hlint-yaml: .hlint.yaml

constraint-set cffi
  constraints: aeson +cffi
  tests:       True
  run-tests:   True

constraint-set ordered-keymap
  constraints: aeson +ordered-keymap
  tests:       True
  run-tests:   True
